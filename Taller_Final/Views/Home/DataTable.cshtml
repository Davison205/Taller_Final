<h1>Registro de Productos</h1>

<!-- /.card -->
<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-outline-primary btn-block"><i class="fas fa-save"></i> Agregar Producto</button>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table id="exdatatable" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="table-primary" scope="col">ID</th>
                    <th class="table-primary" scope="col">Nombre producto</th>
                    <th class="table-primary" scope="col">Categoría</th>
                    <th class="table-primary" scope="col">Precio</th>
                    <th class="table-primary" scope="col">Fecha ingreso</th>
                    <th class="table-primary" scope="col">Stock</th>
                    <th class="table-primary" scope="col">Funcionalidades</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>F-ID_VALUE</td>
                    <td>H-NAME_VALUE</td>
                    <td>A-CAT_VALUE</td>
                    <td>D-PRICE_VALUE</td>
                    <td>B-DATE_VALUE</td>
                    <td>L-STOCK_VALUE</td>
                    <td>
                        <a class="btn btn-info ml-3">
                            <i class="far fa-edit"></i>
                        </a>
                        <a class="btn btn-danger ml-3">
                            <i class="fas fa-trash"></i>
                        </a>
                    </td>
                </tr>
                <tr>
                    <td>V-ID_VALUE</td>
                    <td>P-NAME_VALUE</td>
                    <td>T-CAT_VALUE</td>
                    <td>LL-PRICE_VALUE</td>
                    <td>Z-DATE_VALUE</td>
                    <td>E-STOCK_VALUE</td>
                    <td>
                        <a class="btn btn-info ml-3">
                            <i class="far fa-edit"></i>
                        </a>
                        <a class="btn btn-danger ml-3">
                            <i class="fas fa-trash"></i>
                        </a>
                    </td>
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <th class="table-info" colspan="7"></th>
                </tr>
            </tfoot>
        </table>
    </div>
    <!-- /.card-body -->
</div>
<!-- /.card -->